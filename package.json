{
  "name": "rsbuild-plugin-i18next-extractor",
  "version": "0.1.1",
  "description": "A Rsbuild plugin for extracting i18n translations using i18next-cli",
  "keywords": [
    "rsbuild",
    "rspack",
    "i18n",
    "i18next",
    "i18next-cli",
    "extractor"
  ],
  "license": "MIT",
  "author": {
    "name": "Hengchang Lu",
    "email": "luhengchan228@gmail.com"
  },
  "repository": "https://github.com/rstackjs/rsbuild-plugin-i18next-extractor",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.map",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:write": "biome check . --write"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "dependencies": {
    "i18next-cli": "1.33.5",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.9",
    "@playwright/test": "^1.50.0",
    "@rsbuild/core": "^1.6.15",
    "@types/node": "^22.10.2",
    "i18next": "25.7.3",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.0.0"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.15.0"
}
